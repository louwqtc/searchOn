{
  "name": "Search On",
  "description": "Type 'so' plus a search term into the Omnibox to open search in current tab.",
  "version": "1.0",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "omnibox": { "keyword" : "so" },
  "manifest_version": 2,
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/newtab_search16.png",
      "32": "./images/newtab_search32.png",
      "48": "./images/newtab_search48.png",
      "128": "./images/newtab_search128.png"
      }
  },
  "options_page": "options.html",
  "icons": {
    "16": "./images/newtab_search16.png",
    "32": "./images/newtab_search32.png",
    "48": "./images/newtab_search48.png",
    "128": "./images/newtab_search128.png"
  }
}